+    clauseOps.loopLBVar.push_back(rewriter.clone(*lbOp)->getResult(0));
+    clauseOps.loopUBVar.push_back(rewriter.clone(*ubOp)->getResult(0));
+    clauseOps.loopStepVar.push_back(rewriter.clone(*stepOp)->getResult(0));
